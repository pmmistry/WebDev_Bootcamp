db2 SELECT "'MEDICATION'" AS 'RESOURCE_TYPE', DB2INST1.MEDICATION_LOGICAL_RESOURCES.LOGICAL_ID, DB2INST1.MEDICATION_RESOURCES.IS_DELETED, DB2INST1.MEDICATION_RESOURCES.VERSION_ID, DB2INST1.MEDICATION_RESOURCES.LAST_UPDATED FROM DB2INST1.MEDICATION_LOGICAL_RESOURCES JOIN DB2INST1.MEDICATION_RESOURCES ON DB2INST1.MEDICATION_LOGICAL_RESOURCES.LOGICAL_RESOURCE_ID = DB2INST1.MEDICATION_RESOURCES.LOGICAL_RESOURCE_ID WHERE DB2INST1.MEDICATION_LOGICAL_RESOURCES.LOGICAL_ID = "'<logical_id>'" ORDER BY DB2INST1.MEDICATION_RESOURCES.LAST_UPDATED ASC 